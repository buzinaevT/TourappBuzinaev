<Window x:Class="TourappBuzinaev.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TourappBuzinaev"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="logo1.jpg" HorizontalAlignment="Left"></Image>
        <TextBox HorizontalAlignment="Center" Text="Some text idk" VerticalAlignment="Center" FontSize="30"/>
        <Grid Background="#bae3e8" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" Name="MainFrame" ContentRendered="MF_ContentRendered"></Frame>
        <Button Content="Back" Name="BtnBack"  HorizontalAlignment="Right" Click="BtnBack_Click"/>
    </Grid>
</Window>
